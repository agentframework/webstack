{
  "name": "{{ name }}",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "description": "{{ description }}",
  "author": "{{ author }}",
  "scripts": {
    "serv": "node_modules/.bin/nodemon --config tools/nodemon.standalone.json",
    "clean": "rm -rf bin client coverage logs public",
    "lint": "standard --pretty",
    "build": "npm run build:clean && npm run build:tsc && npm run build:webpack || exit 0",
    "build:clean": "rm -rf ./bin && rm -rf ./public && rm -rf ./client",
    "build:tsc": "node_modules/.bin/tsc -p tsconfig.json --noEmit",
    "build:webpack": "node_modules/.bin/webpack --display-error-details --config tools/webpack.build.config.ts"
  },
  "dependencies": {
    "agentframework": "~0",
    "agentstack": "~0",
    "agentstack-express": "~0",
    "agentstack-mongodb": "~0",
    "agentstack-router": "~0",
    "axios": "~0",
    "body-parser": "~1",
    "compression": "~1",
    "cookie-parser": "~1",
    "express": "5.0.0-alpha.5",
    "helmet": "~3",
    "jsonwebtoken": "~7",
    "mongodb-core": "~2",
    "response-time": "~2",
    "router": "~1"
  },
  "devDependencies": {
    "@types/express": "latest",
    "@types/jasmine": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/node": "latest",
    "babel-core": "latest",
    "babel-loader": "latest",
    "babel-preset-babili": "latest",
    "css-loader": "git://github.com/e2tox/css-loader.git",
    "extract-text-webpack-plugin": "latest",
    "file-loader": "latest",
    "html-webpack-plugin": "latest",
    "istanbul": "next",
    "jasmine": "latest",
    "jasmine-spec-reporter": "latest",
    "nodemon": "latest",
    "optimize-css-assets-webpack-plugin": "latest",
    "ts-loader": "latest",
    "ts-node": "latest",
    "typescript": "latest",
    "typescript-standard": "latest",
    "vue": "latest",
    "vue-loader": "latest",
    "vue-router": "latest",
    "vue-template-compiler": "latest",
    "vue-ts-loader": "latest",
    "vue-typescript-import-dts": "latest",
    "vuex": "latest",
    "webpack": "latest",
    "webpack-bundle-analyzer": "latest",
    "webpack-dev-middleware": "latest",
    "webpack-hot-middleware": "latest"
  }
}
